.subNews-wrapper {
    margin-top: 18rpx;
    background-color: #ffffff;
    box-sizing: border-box;
}
.subNews-wrapper .subNews-subTitle {
    height: 84rpx;
    line-height: 84rpx;
    font-size: 30rpx;
    padding-left: 40rpx;
    font-weight: 600;
    color: #000000;
    position: relative;
}

.subNews-subTitle::after {
    content: "";
    position: absolute;
    width: 200%;
    height: 200%;
    top: 0;
    left: 0;
    border-bottom: 1rpx solid rgba(177, 177, 177, 0.6);
    transform: scale(0.5);
    transform-origin: 0 0;
}
.subNews-branch {
    width: 750rpx;
    padding-left: 40rpx;
    padding-right: 40rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    position: relative;
}
.subNews-branch::after {
    content: "";
    position: absolute;
    width: 178.6667%;
    height: 200%;
    top: 0;
    left: 40rpx;
    border-bottom: 1rpx solid rgba(177, 177, 177, 0.6);
    transform: scale(0.5);
    transform-origin: 0 0;
}

.subNews-branch .subNews-branch-time {
    height: 77rpx;
    font-size: 24rpx;
    line-height: 77rpx;
    color: #a7a7ac;
}
.subNews-branch .subNews-branch-content {
    font-size: 30rpx;
    line-height: 47rpx;
    width: 100%;
}
.subNews-branch .subNews-branch-img {
    background-color: yellow;
    height: 200rpx;  
}
.subNews-branch .subNews-branch-from {
    box-sizing: border-box;
    height: 84rpx;
    line-height: 84rpx;
    /* padding-top: 20rpx;
    padding-bottom: 20rpx; */
    font-size: 26rpx;
    color: #a7a7ac;
}
.subNews-branch .subNews-branch-from .formManImg {
    margin-bottom: -10rpx;
    width: 44rpx;
    height: 44rpx;
    background-color: yellow;
    border-radius: 22rpx;
} 
.subNews-branch .subNews-branch-bottom {
    height: 73rpx;
    font-size: 22rpx;
    line-height: 73rpx;
    color: #cdcccc;
}
.subNews-branch .subNews-branch-bottom .dianzanNum{
    margin-right: 68rpx;
}
.subNews-branch .subNews-branch-bottom .fenxiang{
    float: right;
}
